    "@types/react-dom": "16.9.0",
    "@types/react-jss": "^8.6.4",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "2.19.0",
