    "css-loader": "2.1.1",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "farce": "^0.4.1",
    "file-loader": "3.0.1",
    "found": "^0.5.3",
    "found-relay": "^0.8.0",
    "html-webpack-plugin": "4.0.0-beta.5",
    "identity-obj-proxy": "3.0.0",
    "mini-css-extract-plugin": "0.8.0",
    "moment": "^2.24.0",
    "number-to-words": "^1.2.4",
    "pnp-webpack-plugin": "1.5.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "7.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.1",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.3",
    "react-circular-progressbar": "^2.0.3",
    "react-dev-utils": "^9.0.4",
    "react-dom": "^16.13.1",
    "react-jss": "^10.1.1",
    "react-player": "^2.0.1",
    "react-redux": "^7.1.3",
    "react-relay": "^9.0.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-spinners": "^0.8.1",
    "react-spring": "^8.0.27",
    "react-step-progress-bar": "^1.0.3",
    "react-use-dimensions": "^1.2.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "relay-compiler-language-typescript": "^12.0.0",
    "resolve": "1.12.0",
    "resolve-url-loader": "3.1.0",
    "sass-loader": "7.2.0",
    "semver": "6.3.0",
    "style-loader": "1.0.0",
    "terser-webpack-plugin": "1.4.1",
    "ts-pnp": "1.1.4",
    "tslint": "^5.20.0",
    "url-loader": "2.1.0",
    "watchman": "^1.0.0",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "4.3.1"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "startProd": "node server.js",
    "storybook": "start-storybook -p 4001",
    "storybook-build": "build-storybook -o ./storybook_build",
    "lint": "eslint . --ext .ts,.tsx",
    "precom": "git diff --exit-code --no-index ../delegate/src/sharedComponents ../manager/src/sharedComponents",
    "relay": "relay-compiler --src ./src --schema ./src/api/schema.graphql --language typescript --artifactDirectory ./src/__generated__",
    "relayWatch": "nodemon --exec 'yarn run relay' --ignore 'server.js' -e ts,tsx,graphql"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run precom"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "relay",
        {
          "artifactDirectory": "./src/__generated__"
        }
      ]
    ]
  },
  "devDependencies": {
    "@storybook/addon-docs": "^5.3.17",
    "@storybook/addon-info": "^5.3.17",
    "@storybook/addon-knobs": "^5.3.17",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^5.2.1",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^25.1.4",
    "@types/node": "12.7.5",
    "@types/react": "^16.9.3",
    "@types/react-dom": "16.9.0",
    "@types/react-redux": "^7.1.7",
    "@types/react-relay": "^7.0.3",
    "@types/react-router-dom": "^.1.0",
    "@types/relay-runtime": "^8.0.8",
    "@types/storybook__react": "4.0.2",
    "@typescript-eslint/eslint-plugin": "2.25.0",
    "@typescript-eslint/parser": "2.25.0",
    "awesome-typescript-loader": "5.2.1",
    "babel-eslint": "10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.1.0",
    "babel-plugin-named-asset-import": "^0.3.4",
    "babel-plugin-relay": "^9.0.0",
    "babel-preset-react-app": "9.1.2",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "5.0.2",
    "eslint-loader": "3.0.3",
    "eslint-plugin-flowtype": "3.13.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "^1.6.1",
    "fork-ts-checker-webpack-plugin": "^4.1.2",
    "graphql": "^15.0.0",
    "husky": "^4.2.5",
    "jest": "^25.2.2",
    "jest-environment-jsdom-fourteen": "0.1.0",
    "jest-resolve": "24.9.0",
    "jest-watch-typeahead": "0.4.0",
    "nodemon": "^2.0.3",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^2.0.2",
    "prettifier": "^0.4.0",
    "react-docgen-typescript-loader": "^3.7.1",
    "relay-compiler": "^9.0.0",
    "relay-config": "^9.0.0",
    "serve": "^11.3.0",
    "ts-jest": "^25.2.1",
    "ts-loader": "7.0.1",
    "typescript": "3.8.3",
    "webpack": "4.40.2",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "3.2.1"
  }
}
